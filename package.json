{
	"name": "gitd",
	"version": "0.0.1",
	"description": "The easiest way to download particular directory of any git repository.",
	"bin": {
		"gitd": "src/cli.mjs"
	},
	"scripts": {
		"test": "jest --config=./jest.config.json",
		"test:m": "majestic",
		"doc": "jsdoc -R README.md"
	},
	"keywords": [
		"javascript",
		"node",
		"cli",
		"git"
	],
	"author": "K0d1Lu",
	"license": "ISC",
	"dependencies": {
		"shelljs": "^0.8.4",
		"yargs": "^16.1.1"
	},
	"devDependencies": {
		"eslint": "^7.14.0",
		"eslint-config-prettier": "^6.15.0",
		"eslint-plugin-jest": "^24.1.3",
		"eslint-plugin-prettier": "^3.2.0",
		"eslint-plugin-standard": "^5.0.0",
		"husky": "^4.3.0",
		"jest": "^26.6.3",
		"jsdoc": "^3.6.6",
		"lint-staged": "^10.5.3",
		"majestic": "^1.8.0",
		"prettier": "^2.2.1",
		"pretty-quick": "^3.1.0"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js, mjs}": [
			"eslint --fix",
			"pretty-quick --staged"
		]
	}
}
